# 生产环境配置
NODE_ENV=production

# API基础URL - 生产环境直接请求后端域名
VUE_APP_API_BASE_URL=https://ms.baiyanai.cn

# 图片资源基础路径
VUE_APP_IMAGES_PATH=/images

# 启用性能监控
VUE_APP_ENABLE_PERFORMANCE=true

# 启用错误监控
VUE_APP_ENABLE_ERROR_TRACKING=true

# 应用标题
VUE_APP_TITLE=百色农林大数据中心

# 构建时间戳
VUE_APP_BUILD_TIME=BUILD_TIMESTAMP